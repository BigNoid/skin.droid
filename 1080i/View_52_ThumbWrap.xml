<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="View_52_ThumbWrap">
        <control type="group">
            <visible>Control.isVisible(52)</visible>
            <posy>168</posy>
            <control type="wraplist" id="52">
                <posx>0</posx>
                <posy>362</posy>
                <width>1920</width>
                <height>680</height>
                <onleft>52</onleft>
                <onright>52</onright>
                <onup>9010</onup>
                <ondown>60</ondown>
                <pagecontrol>60</pagecontrol>
                <focusposition>1</focusposition>
                <orientation>horizontal</orientation>
                <viewtype label="Landscape">bigwrap</viewtype>
                <scrolltime tween="quadratic">300</scrolltime>
                <visible>Container.Content(movies) | Container.Content(tvshows) | Container.Content(episodes)</visible>
                <itemlayout width="642" height="680">
                    <control type="group">
                        <visible>IsEmpty(ListItem.Art(landscape)) + IsEmpty(ListItem.Art(clearlogo))</visible>
                        <control type="label">
                            <posx>321</posx>
                            <posy>-18</posy>
                            <width>642</width>
                            <height>60</height>
                            <align>center</align>
                            <font>Small_Caps</font>
                            <textcolor>White2</textcolor>
                            <shadowcolor>Black</shadowcolor>
                            <label>$INFO[ListItem.Label]</label>
                            <textoffsetx>30</textoffsetx>
                        </control>
                    </control>
                    <control type="image">
                        <width>642</width>
                        <height>382</height>
                        <aspectratio>scale</aspectratio>
                        <texture fallback="DefaultVideoCover.png">$VAR[LandscapeThumbVar]</texture>
                        <bordertexture border="21">thumbs/thumb_shadow.png</bordertexture>
                        <bordersize>21</bordersize>
                    </control>
                    <control type="image">
                        <width>642</width>
                        <height>382</height>
                        <aspectratio>keep</aspectratio>
                        <texture>$INFO[ListItem.Art(clearlogo)]</texture>
                        <bordersize>21</bordersize>
                        <visible>IsEmpty(ListItem.Art(landscape))</visible>
                    </control>
                    <control type="image">
                        <posx>500</posx>
                        <posy>240</posy>
                        <width>142</width>
                        <height>142</height>
                        <texture>$INFO[ListItem.Overlay,common/]</texture>
                        <aspectratio align="center" aligny="bottom">keep</aspectratio>
                        <bordersize>21</bordersize>
                    </control>
                    <control type="image">
                        <posy>361</posy>
                        <width>642</width>
                        <height>382</height>
                        <aspectratio>keep</aspectratio>
                        <texture flipy="true" diffuse="views/landscape/mask_reflect.png">$INFO[ListItem.Art(clearlogo)]</texture>
                        <bordersize>21</bordersize>
                        <visible>IsEmpty(ListItem.Art(landscape))</visible>
                    </control>
                    <control type="image">
                        <posx>500</posx>
                        <posy>341</posy>
                        <width>142</width>
                        <height>142</height>
                        <aspectratio scalediffuse="false">stretch</aspectratio>
                        <texture flipy="true" diffuse="views/landscape/mask_reflect.png">$INFO[ListItem.Overlay,common/]</texture>
                        <aspectratio align="center" aligny="bottom">keep</aspectratio>
                        <bordersize>21</bordersize>
                    </control>
                    <control type="image">
                        <posx>21</posx>
                        <posy>361</posy>
                        <width>600</width>
                        <height>340</height>
                        <aspectratio>scale</aspectratio>
                        <texture flipy="true" fallback="DefaultVideoCover.png" diffuse="views/landscape/mask_reflect.png">$VAR[LandscapeThumbVar]</texture>
                        <!--<bordersize>25</bordersize>-->
                    </control>
                </itemlayout>
                <focusedlayout width="636" height="680">
                    <control type="group">
                        <posx>20</posx>
                        <posy>44</posy>
                        <animation type="Conditional" reversible="false" condition="true">
                            <effect type="zoom" start="100" end="113" center="auto" easing="out" tween="cubic" time="0" />
                        </animation>
                        <animation type="Focus" reversible="false">
                            <effect type="zoom" start="88.4" end="100" center="auto" easing="out" tween="cubic" time="200" />
                        </animation>
                        <control type="group">
                            <visible>IsEmpty(ListItem.Art(landscape)) + IsEmpty(ListItem.Art(clearlogo))</visible>
                            <control type="label">
                                <posx>300</posx>
                                <posy>-70</posy>
                                <width>642</width>
                                <height>110</height>
                                <align>center</align>
                                <font>Small_Caps</font>
                                <textcolor>White2</textcolor>
                                <shadowcolor>Black</shadowcolor>
                                <label>$INFO[ListItem.Label]</label>
                                <textoffsetx>30</textoffsetx>
                            </control>
                        </control>
                        <control type="image">
                            <posx>-21</posx>
                            <posy>-21</posy>
                            <width>642</width>
                            <height>382</height>
                            <aspectratio>scale</aspectratio>
                            <texture fallback="DefaultVideoCover.png">$VAR[LandscapeThumbVar]</texture>
                            <bordertexture border="21">thumbs/thumb_shadow.png</bordertexture>
                            <bordersize>21</bordersize>
                        </control>
                        <control type="image">
                            <posx>-21</posx>
                            <posy>-21</posy>
                            <width>642</width>
                            <height>382</height>
                            <aspectratio>keep</aspectratio>
                            <texture>$INFO[ListItem.Art(clearlogo)]</texture>
                            <bordersize>21</bordersize>
                            <visible>IsEmpty(ListItem.Art(landscape))</visible>
                        </control>
                        <control type="image">
                            <posx>479</posx>
                            <posy>219</posy>
                            <width>142</width>
                            <height>142</height>
                            <texture>$INFO[ListItem.Overlay,common/]</texture>
                            <aspectratio align="center" aligny="bottom">keep</aspectratio>
                            <bordersize>21</bordersize>
                        </control>
                        <control type="image">
                            <posx>-21</posx>
                            <posy>340</posy>
                            <width>642</width>
                            <height>382</height>
                            <aspectratio>keep</aspectratio>
                            <texture flipy="true" diffuse="views/landscape/mask_reflect.png">$INFO[ListItem.Art(clearlogo)]</texture>
                            <bordersize>21</bordersize>
                            <visible>IsEmpty(ListItem.Art(landscape))</visible>
                        </control>
                        <control type="image">
                            <posx>479</posx>
                            <posy>320</posy>
                            <width>142</width>
                            <height>142</height>
                            <aspectratio scalediffuse="false">stretch</aspectratio>
                            <texture flipy="true" diffuse="views/landscape/mask_reflect.png">$INFO[ListItem.Overlay,common/]</texture>
                            <aspectratio align="center" aligny="bottom">keep</aspectratio>
                            <bordersize>21</bordersize>
                        </control>
                        <control type="image">
                            <posy>340</posy>
                            <width>600</width>
                            <height>340</height>
                            <aspectratio>scale</aspectratio>
                            <texture flipy="true" fallback="DefaultVideoCover.png" diffuse="views/landscape/mask_reflect.png">$VAR[LandscapeThumbVar]</texture>
                        </control>
                    </control>
                </focusedlayout>
            </control>
        </control>
    </include>
</includes>
