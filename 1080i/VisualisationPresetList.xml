                            <align>right</align>
                            <label>$INFO[ListItem.Label2]</label>
                            <textoffsetx>30</textoffsetx>
                        </control>
                    </control>
                </focusedlayout>
			</control>

            <control type="button">
                <posx>888</posx>
                <posy>72</posy>
                <width>72</width>
                <height>72</height>
                <texturenofocus border="3">dialogs/toast_back_blue.png</texturenofocus>
                <texturefocus border="8">dialogs/button_focus.png</texturefocus>
