                    <control type="image">
                        <width>888</width>
                        <height>72</height>
                        <include>toast_back</include>
                    </control>
                    <control type="label">
                        <width>888</width>
                        <height>72</height>
                        <font>Small</font>
                        <label>$INFO[ListItem.Label]</label>
