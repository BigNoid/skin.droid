				<posx>0</posx>
				<posy>0</posy>
				<width>960</width>
                <height>72</height>
				<label>31032</label>
				<font>Medium</font>
				<textcolor>$VAR[ThemeColorVar]</textcolor>
                <textoffsetx>30</textoffsetx>
            </control>

			<control type="list" id="2">
