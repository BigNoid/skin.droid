                    </control>
                    <control type="label">
                        <posx>888</posx>
                        <width>888</width>
                        <height>72</height>
                        <font>Small</font>
                        <align>right</align>
                        <label>$INFO[ListItem.Label2]</label>
                        <textoffsetx>30</textoffsetx>
                    </control>
                </itemlayout>
				<focusedlayout height="72">
                    <control type="group">
                        <visible>!Control.HasFocus(2)</visible>
                        <control type="image">
                            <width>888</width>
                            <height>72</height>
                            <include>toast_back</include>
                        </control>
                        <control type="label">
                            <width>888</width>
                            <height>72</height>
