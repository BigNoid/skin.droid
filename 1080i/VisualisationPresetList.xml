                        <control type="label">
                            <posx>888</posx>
                            <width>888</width>
                            <height>72</height>
                            <font>Small</font>
                            <align>right</align>
                            <label>$INFO[ListItem.Label2]</label>
                            <textoffsetx>30</textoffsetx>
                            <textcolor>White2</textcolor>
                        </control>
                    </control>
                    <control type="group">
                        <visible>Control.HasFocus(2)</visible>
                        <control type="image">
                            <width>888</width>
                            <height>72</height>
                            <texture border="8">dialogs/button_focus.png</texture>
                        </control>
                        <control type="label">
                            <width>888</width>
                            <height>72</height>
                            <font>Small</font>
                            <label>$INFO[ListItem.Label]</label>
                            <textoffsetx>30</textoffsetx>
                        </control>
                        <control type="label">
                            <posx>888</posx>
                            <width>888</width>
                            <height>72</height>
